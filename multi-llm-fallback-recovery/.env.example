# .env.example
# Environment variables for Pipecat Multi-LLM Fallback Example

# Ollama (Priority 1 - Local)
OLLAMA_URL="http://localhost:11434"
OLLAMA_URL_V1="http://localhost:11434/v1"
OLLAMA_MODEL="llama3:latest"

# Google Gemini (Priority 2 - Cloud)
GEMINI_API_KEY="your_gemini_api_key_here"
GEMINI_MODEL="gemini-2.0-flash-exp"

# OpenAI (Priority 3 - Cloud)
OPENAI_API_KEY="your_openai_api_key_here"
OPENAI_MODEL="gpt-4o-mini"

# DeepSeeker (Priority 4 - Cloud) - Optional
DEEPSEEK_API_KEY="your_deepseek_api_key_here"
DEEPSEEK_MODEL="deepseek-3.5-turbo"

# Deepgram (Speech to Text)
DEEPGRAM_API_KEY="your_deepgram_api_key_here"

# Cartesia (Text to Speech)
CARTESIA_API_KEY="your_cartesia_api_key_here"

# Daily (WebRTC)
DAILY_API_KEY="your_daily_api_key_here"
